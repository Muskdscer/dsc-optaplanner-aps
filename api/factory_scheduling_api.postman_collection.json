        {
          "name": "更新工作中心",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"加工中心1-更新\",\n  \"type\": \"CNC\",\n  \"capacity\": 2\n}"
