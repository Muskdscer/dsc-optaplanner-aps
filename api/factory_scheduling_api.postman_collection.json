  "item": [
    {
      "name": "调度管理",
      "description": "调度相关接口",
      "item": [
        {
          "name": "获取最佳解决方案",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/scheduling/solution/:problemId",
              "host": ["{{baseUrl}}"] ,
              "path": ["api", "scheduling", "solution", ":problemId"],
              "variable": [
                {
                  "key": "problemId",
                  "value": "1",
                  "description": "问题ID，指定要获取解决方案的调度任务"
                }
              ]
            },
            "description": "获取指定问题ID的当前最佳调度解决方案，包含所有已优化的时间槽安排"
          },
          "response": []
        },
        {
          "name": "获取求解状态",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/scheduling/status/:problemId",
              "host": ["{{baseUrl}}"] ,
              "path": ["api", "scheduling", "status", ":problemId"],
              "variable": [
                {
                  "key": "problemId",
                  "value": "1",
                  "description": "问题ID，指定要查询状态的调度任务"
                }
              ]
            },
            "description": "获取指定问题ID的当前调度任务的求解状态，如正在求解、已完成、未开始等"
          },
          "response": []
        },
        {
          "name": "删除所有调度数据",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/scheduling/deleteAll",
              "host": ["{{baseUrl}}"] ,
              "path": ["api", "scheduling", "deleteAll"]
            },
            "description": "删除系统中的所有调度相关数据，通常用于测试或重置环境"
          },
          "response": []
        }
      ]
    },
    {
