            "url": {
              "raw": "{{baseUrl}}/api/machines/:id",
              "host": ["{{baseUrl}}"] ,
              "path": ["api", "machines", ":id"],
              "variable": [
                {
                  "key": "id",
                  "value": "M001",
                  "description": "工作中心ID"
                }
              ]
            },
            "description": "根据指定的ID更新工作中心（设备/机器）的信息"
          },
          "response": []
        }
      ]
